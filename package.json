{
  "name": "vant-next-components",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --config ./build/base.config.js",
    "build:doc": "vite build --config ./build/doc.config.js",
    "build:lib": "vite build --config ./build/lib.config.js",
    "release": "release-it && git push --follow-tags origin main"
  },
  "peerDependencies": {
    "vant": "^3.4.2",
    "vue": "^3.2.29"
  },
  "devDependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "highlight.js": "^11.4.0",
    "less": "^4.1.2",
    "vant": "^3.4.2",
    "vite": "^2.7.2",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-md": "^0.11.7",
    "vue": "^3.2.29",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.31.1"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "push": false,
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}"
    }
  }
}
